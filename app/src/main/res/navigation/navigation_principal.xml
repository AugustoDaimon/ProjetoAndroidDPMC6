<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_principal"
    app:startDestination="@id/principalFragment">

    <fragment
        android:id="@+id/principalFragment"
        android:name="com.example.nutzen.PratosDoDia.PrincipalFragment"
        android:label="fragment_principal"
        tools:layout="@layout/fragment_principal" >
        <action
            android:id="@+id/action_principalFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.nutzen.PerfilUser.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_exercicios22Fragment"
            app:destination="@id/exercicios22Fragment" />
    </fragment>
    <fragment
        android:id="@+id/exercicios22Fragment"
        android:name="com.example.nutzen.Exercicios22Fragment"
        android:label="fragment_exercicios22"
        tools:layout="@layout/fragment_exercicios22" />
</navigation>